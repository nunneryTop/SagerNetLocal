<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorPickerPaletteFlex">
        <attr name="cpl_currentColor" format="reference" />

        <attr name="cpl_colors" format="reference" />

        <attr name="cpl_sortColors" format="boolean|reference" />

        <attr name="cpl_colorDescriptions" format="reference" />

        <attr name="cpl_columns" format="integer|reference" />

        <attr name="cpl_size" format="enum|reference">
            <enum name="large" value="1" />
            <enum name="small" value="2" />
        </attr>
    </declare-styleable>
</resources>